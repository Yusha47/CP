
#include <bits/stdc++.h>
 
using namespace std;
 
int main() {
 
	ios::sync_with_stdio(false);
	cin.tie(0);
 
	int n, m;
	cin >> n >> m;
 
	if(n == 1) {
		if(m != 0)
			cout << -1 << endl;
		else
			cout << "1 2" << endl;
		return 0;
	}
 
	if(m < 0 || m > n - 2) {
		cout << -1 << endl;
		return 0;
	}
 
	if(m == 0) {
		for(int i=0, s = 0; i<n; ++i, s+= 2) {
			cout << s << " " << s + 1 << endl;
		}
		return 0;
	}
 
	if(m <= n - 2) {
		int s = 2;
		cout << 1 << " " << 2*(m + 1) + 5 << "\n"; 
		for(int cnt=0; cnt<m+1; s+=2, ++cnt) {
			cout << s << " " << s + 1 << "\n";
		}
		s += 100;
		for(int k=0; k < n-m-2; ++k, ++s) {
			cout << s + 1 + k << " " << s + k + 2 << endl;
		}
	}
 
	return 0;
}
